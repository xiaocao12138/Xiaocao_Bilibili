<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- CircleImageView -->
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
    </declare-styleable>

    <attr name="entrySummaries" format="reference" />

    <declare-styleable name="IjkListPreference">
        <attr name="entrySummaries" />
    </declare-styleable>

    <declare-styleable name="progress">
        <attr name="sweepArc" format="integer"/>
        <attr name="progressWidth" format="dimension"/>
        <attr name="bgColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="srcColor" format="color"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

</resources>